<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ucucite.edu.getfits">

    <application
        android:allowBackup="true"
        android:icon="@drawable/getfit"
        android:label="@string/app_name"
        android:roundIcon="@drawable/getfit"
        android:supportsRtl="true"
        android:theme="@style/Theme.GetFits">
        <activity android:name=".Receipt"></activity>
        <activity android:name=".list"></activity>
        <activity android:name=".AboutUs" />
        <activity android:name=".start" />
        <activity android:name=".homepage" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".RegisterUser" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>